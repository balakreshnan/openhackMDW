{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "openhackadfhacker6"
		},
		"ADLSStore_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSStore'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"CloudStreamingdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreamingdb'"
		},
		"Moviescosmosdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Moviescosmosdb'"
		},
		"Fourthcoffee_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Fourthcoffee'"
		},
		"fctranscation_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'fctranscation'"
		},
		"ADLSStore_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://openhackdatalake.dfs.core.windows.net"
		},
		"Fourthcoffee_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"Fourthcoffee_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"fctranscation_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Transactions_2018"
		},
		"fctranscation_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSStore_accountKey": {
						"value": "[parameters('ADLSStore_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"CloudStreamingdb_connectionString": {
						"value": "[parameters('CloudStreamingdb_connectionString')]"
					},
					"Moviescosmosdb_connectionString": {
						"value": "[parameters('Moviescosmosdb_connectionString')]"
					},
					"ADLSStore_properties_typeProperties_url": {
						"value": "[parameters('ADLSStore_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Fourthcoffee_password": {
						"value": "[parameters('Fourthcoffee_password')]"
					},
					"fctranscation_password": {
						"value": "[parameters('fctranscation_password')]"
					},
					"Fourthcoffee_properties_typeProperties_host": {
						"value": "[parameters('Fourthcoffee_properties_typeProperties_host')]"
					},
					"Fourthcoffee_properties_typeProperties_userId": {
						"value": "[parameters('Fourthcoffee_properties_typeProperties_userId')]"
					},
					"fctranscation_properties_typeProperties_host": {
						"value": "[parameters('fctranscation_properties_typeProperties_host')]"
					},
					"fctranscation_properties_typeProperties_userId": {
						"value": "[parameters('fctranscation_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}